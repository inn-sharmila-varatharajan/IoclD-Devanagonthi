<style>
    *,
    *::after,
    *::before {
        box-sizing: inherit;
        margin: 0;
        padding: 0;
    }

    html {
        font-size: 62.5%;
    }

    body {
        box-sizing: border-box;
        font-family: 'Open Sans', sans-serif;
        position: relative;
    }


    .card-front__heading {
        font-size: 1.5rem;
        margin-top: .25rem;
    }

    .inside-page__heading {
        padding-bottom: 1rem;
        width: 100%;
    }

    .card-front__tp {
        align-items: center;
        clip-path: polygon(0 0, 100% 0, 100% 90%, 57% 90%, 50% 100%, 43% 90%, 0 90%);
        display: flex;
        flex-direction: column;
        height: 12rem;
        justify-content: center;
        padding: .75rem;
    }

    .card-front__text-price {
        font-size: 1.2rem;
        margin-top: -.2rem;
    }

    .inside-page__text {
        color: #333;
    }


    .card-front__icon {
        fill: #fafbfa;
        font-size: 3vw;
        height: 3.25rem;
        margin-top: -.5rem;
        width: 3.25rem;
    }

    .main {
        display: flex;
        flex-direction: column;
        justify-content: center;
        position: absolute;
        width: 100%
    }

    .card-area {
        align-items: center;
        display: flex;
        flex-wrap: nowrap;
        height: 100%;
        justify-content: space-evenly;
        padding: 1rem;
    }


    .card-section {
        align-items: center;
        display: flex;
        height: 100%;
        justify-content: center;
        width: 100%;
    }

    .cardcard {
        background-color: rgba(0,0,0, .05);
        box-shadow: -.1rem 1.7rem 6.6rem -3.2rem rgba(0,0,0,0.5);
        height: 15rem;
        transition: all 1s ease;
        width: 15rem;
        
       
    }

    .flip-card {
        height: 15rem;
        perspective: 100rem;
        transition: all 1s ease;
        visibility: hidden;
        width: 15rem;
        z-index: 100;
    }

        .flip-card > * {
            visibility: visible;
        }

    .flip-card__container {
        height: 100%;
        right: 0;
        transform-origin: left;
        transform-style: preserve-3d;
        transition: all 1s ease;
        width: 100%;
    }

    .card-front,
    .card-back {
        backface-visibility: hidden;
        height: 100%;
        left: 0;
        position: absolute;
        top: 0;
        width: 100%;
    }

    .card-front {
        background-color: #fafbfa;
        height: 15rem;
        width: 15rem;
    }

    .card-front__tp {
        align-items: center;
        clip-path: polygon(0 0, 100% 0, 100% 90%, 57% 90%, 50% 100%, 43% 90%, 0 90%);
        display: flex;
        height: 12rem;
        justify-content: center;
        padding: .75rem;
    }

    .card-front__tp--city {
        background: linear-gradient( to bottom, #ffb3e6, #ffb3e6 );
    }

    .card-front__tp--city1 {
        background: linear-gradient( to bottom, #ccccff, #ccccff );
    }

    .card-front__tp--ski {
        background: linear-gradient( to bottom, #b3ffda, #b3ffda );
    }


    .card-front__tp--beach {
        background: linear-gradient( to bottom, #ffd9b3, #ffd9b3 );
    }

    .card-front__tp--beach1 {
        background: linear-gradient( to bottom, #e6ff99, #e6ff99 );
    }

    .card-front__tp--camping {
        background: linear-gradient( to bottom, #00db93, #00b97d );
    }

    .card-front__bt {
        align-items: center;
        display: flex;
        justify-content: center;
        margin-top:-10px;
    }

    .card-back {
        background-color: #fafbfa;
        transform: rotateY(180deg);
    }

    @@keyframes scaleIn {
        from {
            transform: scale(.5, .5);
            opacity: .5;
        }

        to {
            transform: scale(2.5, 2.5);
            opacity: 0;
        }
    }

    .circle {
        border-radius: 50%;
        width: 70px;
        height: 70px;
        position: absolute;
        opacity: 0;
        animation: scaleIn 4s infinite cubic-bezier(.36, .11, .89, .32);
    }

    .inline {
        display: inline-block;
    }

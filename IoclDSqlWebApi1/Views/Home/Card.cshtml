                integrity="sha512-uto9mlQzrs59VwILcLiRYeLKPPbS/bT71da/OEBYEwcdNUk8jYIy+D176RYoop1Da+f9mvkYrmj5MCLZWEtQuA=="
                crossorigin="anonymous"
                referrerpolicy="no-referrer"></script>

        <!-- JavaScript Bundle with Popper -->

        <script type="text/javascript">
            let selecteddate,selectedevice;
           var weburl= '@System.Configuration.ConfigurationManager.AppSettings["URL"]';
            var acc = document.getElementsByClassName("accordion");
            var i;
            var spinner = $('#loader');

            async function Getproductioncount(value) {

                let url = weburl + "api/GetCardController/Getproductioncount/" + value;
                //let url = " http://188.42.97.98:90/api/GetCardController/GetRPHValues/2022-08-09 ";

                axios.get(url).then

                    (data => {

                        console.log(data.data);
                        var row = '', row1 = '', row2 = '', row3 = '';
                        if (data.data.length == 0) {

                            return;
                        }

                        document.getElementById("totalcount").innerHTML =   "Total Production Count: "+data.data[0]["Total"];
                      //  alert(data.data[0]["Total"]);


                    }).catch(err => alert(err));
            }
            async function onchangeddetails(value) {

                let url = weburl +"api/GetCardController/GetDeviceid1/" + value;
                //let url = " http://188.42.97.98:90/api/GetCardController/GetRPHValues/2022-08-09 ";

                axios.get(url).then

                (data => {

                    console.log(data.data);
                    var row = '', row1 = '', row2 = '', row3 = '';
                    if (data.data.length == 0) {

                        return;
                    }

                    for (var i = 0; i < data.data.length; i++) {
                        let obj = data.data[i];
                        var fillcolor = "#f1f1f1";
                        var hh = obj.DeviceName.split('-');
                        if (hh[0] == "Carousel1 ") {
                            fillcolor = "#f1f1f1";

                            row += `
                                <div class="column">
                                <div style="background-color:${fillcolor}" class="card">
                          <p style="font-size:16px;height:70px;">${obj.DeviceName + " [" + obj.DeviceId + "]"}</p>

          <div class="inline">

          <button class="button button1">${obj.eventcode40}</button>
                                 <button class="button button2" >0</button>
        </div>
        </div>
                               </div></div>`;

                        }
                        else if (hh[0] == "Carousel2 ") {
                            fillcolor = "#f1f1f1";

                            row1 += `


                                <div class="column">
                                <div style="background-color:${fillcolor}" class="card">
                          <p style="font-size:16px;height:70px;">${obj.DeviceName + " [" + obj.DeviceId + "]"}</p>

                  <div class="inline">

          <button class="button button1">${obj.eventcode40}</button>
                                 <button class="button button2">0</button>
        </div>
        </div>
                               </div></div>`;

                        }
                        else if (hh[0] == "Carousel3 ") {
                            fillcolor = "#f1f1f1";

                            row2 += `


                                <div class="column">
                                <div style="background-color:${fillcolor}" class="card">
                          <p style="font-size:16px;height:70px;">${obj.DeviceName + " [" + obj.DeviceId + "]"}</p>
                          <div class="inline">
          <button class="button button1">${obj.eventcode40}</button>
                                 <button class="button button2">${obj.eventcode41}</button>
        </div>
        </div>
                               </div></div>`;

                        }
                        else if (hh[0] == "Carousel4 ") {
                            fillcolor = "#f1f1f1";

                            row3 += `


                                <div class="column">
                                <div style="background-color:${fillcolor}" class="card">
                          <p style="font-size:16px;height:70px;">${obj.DeviceName + " [" + obj.DeviceId + "]"}</p>
                          <div class="inline">
          <button class="button button1">${obj.eventcode40}</button>
                                 <button class="button button2">0</button>
        </div>
        </div>
                               </div></div>`;

                        }
                    }
                    document.getElementById("card_binding").innerHTML = row;
                    document.getElementById("card2_binding").innerHTML = row1;
                    document.getElementById("card4_binding").innerHTML = row3;

                    document.getElementById("card3_binding").innerHTML = row2;

                }).catch(err => alert(err));

                afterdataload();

            }



            $('#datechange').change(function () {
                selecteddate = this.value;


               // onchangeddetails(selecteddate);
                getaveragevalues(selecteddate, 17);
                //Getproductioncount(selecteddate);


            });
            $(document).ready(function () {

                var now = new Date();

                var day = ("0" + now.getDate()).slice(-2);
                var month = ("0" + (now.getMonth() + 1)).slice(-2);

                var today = now.getFullYear() + "-" + (month) + "-" + (day);
                //onchangeddetails(today);
                getaveragevalues(today, 17);
                //Getproductioncount(today);
                selectedevice = 17;

                $('#datechange').val(today);
                selecteddate = today;
                datechange.max = new Date().toISOString().split("T")[0];

                $(function () {

                    $("#tabs").tabs({
                        show: { effect: "blind", direction: "right", duration: 300 }
                    });
                    $("#accordion").accordion();

                    var btn = $('#accordion li a');
                    var wrapper = $('#accordion li');

                    $(btn).on('click', function () {
                        $(btn).removeClass('active');
                        $(btn).parent().find('.addon').removeClass('fadein');

                        $(this).addClass('active');
                        $(this).parent().find('.addon').addClass('fadein');
                    });
                });

            });



            function afterdataload() {

                for (i = 0; i < acc.length; i++) {

                    acc[i].addEventListener("click", function () {
                        /* Toggle between adding and removing the "active" class,
                        to highlight the button that controls the panel */
                        this.classList.toggle("active");

                        /* Toggle between hiding and showing the active panel */
                        var panel = this.nextElementSibling;
                        if (panel.style.display === "block") {
                            panel.style.display = "none";
                        } else {
                            panel.style.display = "block";
                        }
                    });
                }
               // acc[0].click();

            }
           function getaveragevalues(value,deviceid)
           {
               selectedevice = deviceid;

               if (deviceid ==26 ) {
                   document.getElementById('cc').style.visibility = 'hidden';
                   document.getElementById('cc1').style.visibility = 'hidden';
                   document.getElementById('cc2').style.visibility = 'hidden';
                   document.getElementById('cc3').style.visibility = 'hidden';
                   document.getElementById('cc4').style.visibility = 'hidden';
                   document.getElementById('cc5').style.visibility = 'hidden';
                   document.getElementById('cc6').style.visibility = 'hidden';
                   document.getElementById('cc7').style.visibility = 'visible';
                   document.getElementById('cc8').style.visibility = 'visible';
             } else {
                   document.getElementById('cc').style.visibility = 'visible';
                   document.getElementById('cc1').style.visibility = 'visible';
                   document.getElementById('cc2').style.visibility = 'visible';
                   document.getElementById('cc3').style.visibility = 'visible';
                   document.getElementById('cc4').style.visibility = 'visible';
                   document.getElementById('cc5').style.visibility = 'visible';
                   document.getElementById('cc6').style.visibility = 'visible';
                   document.getElementById('cc7').style.visibility = 'hidden';
                   document.getElementById('cc8').style.visibility = 'hidden';
               }

               let url = weburl + "api/GetCardController/GetAverage/" + value + "/" + deviceid;
               axios.get(url).then
                   (data => {
                       console.log(data.data);
                       var row = '', row1 = '', row2 = '', row3 = '';

                       if (deviceid == 26) {
                           document.getElementById('scraptext').innerHTML = "ST due-Live Count";
                           scraptext.style.fontSize = "16px";
                           scraptext.style.textAlign = "center";
                           painttext.style.textAlign = "center";

                           document.getElementById('stcount').innerHTML = numberWithCommas((data.data[0]['dc'] == null ? "-" : data.data[0]['dc']));
                           document.getElementById('ptcount').innerHTML = numberWithCommas((data.data[0]['cc'] == null ? "-" : data.data[0]['cc']));
                           return;
                       }


                           if (data.data.length > 0) {

                               document.getElementById('rph').innerHTML = numberWithCommas((data.data[0]['rph'] == null ? "-" : data.data[0]['rph']));
                               // document.getElementById('standard').value = (data.data[0]['standard']);



                               if (data.data[0]['deviation'] > 89) {
                                   //document.getElementById('dot').style.backgroundColor = "#00ff00";
                                   document.getElementById('circle').style.backgroundColor = "#00ff00";
                                   document.getElementById('circle1').style.backgroundColor = "#00ff00";
                                   document.getElementById('circle2').style.backgroundColor = "#00ff00";
                                   document.getElementById('circle3').style.backgroundColor = "#00ff00";
                               }
                               else if (data.data[0]['deviation'] > 70) {

                                   //document.getElementById('dot').style.backgroundColor = "#00FFFF";
                                   document.getElementById('circle').style.backgroundColor = "#00FFFF";
                                   document.getElementById('circle1').style.backgroundColor = "#00FFFF";
                                   document.getElementById('circle2').style.backgroundColor = "#00FFFF";
                                   document.getElementById('circle3').style.backgroundColor = "#00FFFF";


                               }
                               else {

                                   //document.getElementById('dot').style.backgroundColor = "#FF0000";
                                   document.getElementById('circle').style.backgroundColor = "#FF0000";
                                   document.getElementById('circle1').style.backgroundColor = "#FF0000";
                                   document.getElementById('circle2').style.backgroundColor = "#FF0000";
                                   document.getElementById('circle3').style.backgroundColor = "#FF0000";
                               }

                               document.getElementById('deviation').innerHTML = numberWithCommas((data.data[0]['deviation']) == null ? "-" : data.data[0]['rph'] * 16 /*+ "%"*/);
                               document.getElementById('livecount').innerHTML = numberWithCommas((data.data[0]['count']) == null ? "-" : data.data[0]['count']  /*+ "%"*/);
                               document.getElementById('deviation1').innerHTML = numberWithCommas((data.data[0]['deviation']) == null ? "-" : data.data[0]['deviation'] + "%");
                               document.getElementById('dccount').innerHTML = numberWithCommas((data.data[0]['dc']) == null ? "-" : data.data[0]['dc']);
                               document.getElementById('cccount').innerHTML = numberWithCommas((data.data[0]['cc']) == null ? "-" : data.data[0]['cc']);

                               return;
                           }



                   }).catch(err => alert(err))
            }
            function numberWithCommas(x) {
                return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
            }
            function set() {

                let url = weburl + "api/GetCardController/Setvalue/" + document.getElementById('standard').value + "/" + selectedevice;
               axios.get(url).then
                   (data => {
                      // console.log(data.data);

                       if (data.data == 1) {
                           onchangeddetails(selecteddate, selectedevice);
                           getaveragevalues(selecteddate, selectedevice);
                       }


                   }).catch(err => alert(err))
            }
            function bulb_on() {
                document.getElementById('bulb').src = 'https://i.postimg.cc/6QyTynzr/bulb-on.png';
            }
            function bulb_off() {
                document.getElementById('bulb').src = 'https://i.postimg.cc/KjK1wL3c/bulb-off.png';
            }
        </script>

<style>
    @@import url('https://fonts.googleapis.com/css?family=Open Sans:400,700|Montserrat');

    .body {
        margin: 0;
        padding: 0;
        font-family: 'Montserrat';
        font-size: 13px;
        letter-spacing: 1px;
    }

    #tabs {
        width: 100%;
        display: flex;
        position: relative;
        margin: 0 auto;
        margin-top: 10px;
        height: auto;
    }

        #tabs ul {
            position: initial;
            display: inline-block;
            list-style-type: none;
            vertical-align: top;
            width: 15%;
            height: 200px;
        }

            #tabs ul li a {
                position: relative;
                display: block;
                width: 100%;
                text-decoration: none;
                color: #3b3b3b;
                border-top: solid 2px #e9e9e9;
                padding: 20px 5px;
                z-index: 1;
                opacity: .5;
                -webkit-transition: 0.3s ease;
                -moz-transition: 0.3s ease;
                -o-transition: 0.3s ease;
                transition: 0.3s ease;
            }

            #tabs ul li:focus, a:focus {
                outline: none;
            }

            #tabs ul li:nth-child(1) a {
                border-top: none;
            }

            #tabs ul li a.active {
                opacity: 1;
                width: 7pc;
            }

    .para-a {
        font-weight: bold;
        margin-top: 10px;
        padding: 0;
        text-transform: uppercase;
        font-size: 11px;
        letter-spacing: 1px;
        border: none;
        font-family: 'Montserrat';
        color: #2b2b2b;
    }

    .tag {
        position: relative;
        width: 100%;
        margin-left: 10px;
    }

    .icon {
        position: relative;
        display: inline-block;
        width: 50px;
        height: 50px;
        background: #c7c7c7;
        border-radius: 100%;
        vertical-align: middle;
    }

    .block {
        position: absolute;
        width: 18px;
        height: 18px;
        vertical-align: middle;
        border: solid 2px #7b7b7b;
        border-radius: 5px;
        top: 50%;
        left: 50%;
        -webkit-transform: translate(-50%, -50%);
        -moz-transform: translate(-50%, -50%);
        -o-transform: translate(-50%, -50%);
        -ms-transform: translate(-50%, -50%);
        transform: translate(-50%, -50%);
    }

    /*  .circle {
